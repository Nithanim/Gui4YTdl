<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane xmlns="http://javafx.com/javafx/8.0.161-ea" xmlns:fx="http://javafx.com/fxml/1">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <HBox maxHeight="-Infinity" maxWidth="1.7976931348623157E308">
         <children>
            <CheckBox fx:id="chbSpeed" maxHeight="1.7976931348623157E308" mnemonicParsing="false" text="Limit speed">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin></CheckBox>
            <ComboBox fx:id="cobSpeed" disable="true" prefWidth="150.0" promptText="16 KB/s" />
         </children>
      </HBox>
      <HBox fx:id="playlistOptionsPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" />
      <HBox maxHeight="-Infinity" maxWidth="1.7976931348623157E308" GridPane.rowIndex="1">
         <children>
            <CheckBox fx:id="chbIgnoreErrors" maxHeight="1.7976931348623157E308" mnemonicParsing="false" text="Ignore errors">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </CheckBox>
         </children>
      </HBox>
   </children>
</GridPane>
